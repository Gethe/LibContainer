package-as: Backpack

enable-nolib-creation: no

externals:
  libs/LibStub: svn://svn.wowace.com/wow/libstub/mainline/tags/1.0
  libs/Wasabi:
    url: https://github.com/p3lim-wow/Wasabi
    tag: latest

optional-dependencies:
  - wasabi

manual-changelog:
  filename: CHANGELOG.md
  markup-type: markdown

move-folders:
  Backpack/layout_ext: Backpack_Classic
  Backpack/assets: Backpack_Classic/assets

ignore:
  - README.md
